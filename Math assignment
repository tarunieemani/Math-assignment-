import numpy as np
import math
s_temp = int(input('Enter surrounding temp: '))
t1 = 0
t2 = int(input('Second time: '))
temp_1 = int(input('Enter temperature at t1: '))
temp_2 = int(input('Enter temperature at t2: '))
temp_b = int(input('Temperature of body: '))
c = (temp_1 - s_temp)
k = (-1/t2)*(math.log((temp_2-s_temp)/c))
time = (-1/k)*(math.log((temp_b - s_temp)/c))
print(time)
